{
  "name": "app2",
  "description": "superblog",
  "routes": [{
    "from": "/assets/:path",
    "to": "/home/src/assets/{{path}}"
  }, {
    "from": "/js/:filename.js",
    "to": "/bin/engine/js/{{filename}}.js",
    "type": "application/javascript"
  }, {
    "from": "/editor",
    "to": "/bin/engine/editor.html"
  }, {
    "from": "/post",
    "to": "/bin/engine/post.html"
  }, {
    "from": "/",
    "to": "/bin/engine/index.html",
    "type": "text/html"
  }, {
    "from": "/lib/:path",
    "to": "/lib/{{path}}/index.js",
    "type": "application/javascript"
  }]
}
